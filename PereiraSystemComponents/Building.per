)
; MARKET
(defrule
    (up-players-in-game ally > 1)
    (game-time > 120) ; so they dont build it inside
    (building-type-count town-center > 0)
    (building-type-count-total market == 0)
    (or
        (or
            (or
                (wood-amount > 450)
                (food-amount > 900)
            )
            (or
                (gold-amount > 500)
                (stone-amount > 800)
            )
